      "          serviceName: ${4:ingresssvcname}",
      "          servicePort: ${5:ingresssvcport}",
      "  tls:",
      "  - hosts:",
      "    - ${6:ingressfqdntls}",
      "    secretName: ${7:ingresstlsname}"
    ],
    "description": "Code snippet for a Ingress definition"
  },
